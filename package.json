{
  "name": "tsvalidators",
  "version": "0.4.0",
  "description": "tsvalidators",
  "license": "MIT",
  "repository": "",
  "author": {
    "name": "Tim Bavaro",
    "email": "tbavaro@gmail.com",
    "url": "http://timba.me"
  },
  "keywords": [
    ""
  ],
  "files": [
    "lib"
  ],
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "clean": "rimraf lib && rimraf coverage",
    "format": "prettier --write \"{src}/**/*.ts\" es5",
    "lint": "tslint --force --format verbose \"src/**/*.ts\"",
    "prepublishOnly": "npm run build",
    "prebuild": "npm run clean && echo skipping npm run format && npm run lint && echo Using TypeScript && tsc --version",
    "build": "tsc --pretty",
    "test": "jest",
    "coverage": "jest --coverage",
    "watch": "npm run build -- --watch",
    "watch:test": "jest --watch",
    "push": "./node_modules/.bin/tsdevtools deploy"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/jest": "^22.2.3",
    "@types/node": "^8.10.37",
    "coveralls": "^3.0.2",
    "jest": "^23.6.0",
    "prettier": "^1.15.2",
    "rimraf": "^2.0.0",
    "ts-jest": "^23.10.4",
    "ts-node": "^3.2.0",
    "tsdevtools": "github:tbavaro/tsdevtools#dist",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "typescript": "^3.1.6"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "jest": {
    "transform": {
      ".(ts)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts)$",
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "testEnvironment": "node"
  }
}
